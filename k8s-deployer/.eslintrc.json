// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "semi": "off",
    "arrow-parens": "off",
    "no-throw-literal": "off",
    "no-async-promise-executor": "off",
    "no-restricted-syntax": "off"
  }
}
